{
	"name": "Galastri Dark Theme",
	"type": "dark",
	"colors": {
		/* Editor */
		"editor.background": "#161616",
		"editor.foreground": "#ffffff",
			// Editor line number
			"editorLineNumber.activeForeground": "#ffffff",
			"editorLineNumber.foreground": "#464646",
			// Indent guides
			"editorIndentGuide.background": "#ffffff06",
			"editorIndentGuide.activeBackground": "#273d4b9c",
			// Line highlight
			"editor.lineHighlightBackground": "#0099ff1e",
			// Selection color
			"editor.selectionBackground": "#0099ff4f",
			"editor.inactiveSelectionBackground": "#0099ff4f",
			// Highlight similar texts (matches the selection or the word where cursor position is on)
			"editor.selectionHighlightBackground": "#0099ff4f",
			"editor.selectionHighlightBorder": "#0099ff36",
			// Highlight color when searching (CTRL + F)
			"editor.findMatchBackground": "#ffcc23",
			"editor.findMatchHighlightBackground": "#fffa9f",
			// Block folding
			"editor.foldBackground": "#00000000",


		/* Diff Editor */
		"diffEditor.insertedTextBackground": "#15ca8e1f",
		"diffEditor.removedTextBackground": "#ff00552f",
		"diffEditor.diagonalFill": "#839ed81f",

		/* Activity sidebar (Explorer, Source control, etc) */
		"activityBar.background": "#181818",
		"activityBar.activeBorder": "#0099ff62",
		"activityBar.border": "#202020",
		"activityBarBadge.background": "#1568a0",

		/* Sidebar panels (when you open a activity) */
		"sideBar.background": "#181818",
		"sideBar.foreground": "#acacac",
		"sideBar.border": "#222222",
			// Extension button (Install)
			"extensionButton.prominentBackground": "#0cbd82",
			"extensionButton.prominentHoverBackground": "#0ae99f",
			"extensionButton.prominentForeground": "#0b0b0b",
			// List
			"list.hoverBackground": "#373a3b6c",
			"list.focusOutline": "#00000000",
			"list.inactiveSelectionBackground": "#0b476f", // background when unfocus the list item
			"list.inactiveSelectionForeground": "#ffffff", // foreground when unfocus the list item

		/* Title bar */
		"titleBar.activeBackground": "#1a1a1a",
		"titleBar.inactiveBackground": "#1a1a1a",
		"titleBar.border": "#161616",

		/* Minimap */
		"minimap.background": "#181818",
		"minimap.selectionHighlight": "#0099ff",
		"minimap.findMatchHighlight": "#fffbb59a",
		"minimapSlider.background": "#ffffff19",
		"minimapSlider.activeBackground": "#ffffff1e",
		"minimapSlider.hoverBackground": "#ffffff1e",

		/* Menubar */
		"menubar.selectionBackground": "#0b476f",
		"menubar.selectionBorder": "#00000000",

		/* Tabs and tab bar */
		"tab.activeBackground": "#0b476f36",
		"tab.inactiveBackground": "#1a1a1a",
		"tab.hoverBackground": "#1e77b333",
		"tab.border": "#00000000",
		"tab.activeBorderTop": "#103955",
		"editorGroupHeader.tabsBackground": "#1a1a1a",
		"scrollbar.shadow": "#0000001f",

		/* Scroll */
		"scrollbarSlider.background": "#ffffff32",
		"scrollbarSlider.hoverBackground": "#ffffff47",
		"scrollbarSlider.activeBackground": "#ffffff47",
		"editorOverviewRuler.border": "#232323",
		"editorOverviewRuler.background": "#151515",
		"editorOverviewRuler.findMatchForeground": "#fffbb5a4",

		/* Bracket open/close hightlight */
		"editorBracketMatch.background": "#db179a",
		"editorBracketMatch.border": "#00000000",

		/* Bottom panel */
		"panel.border": "#222222",
		"panel.background": "#181818",

		/* Fields */
		"focusBorder": "#0000005b",
		"dropdown.border": "#282a2b",
		"dropdown.background": "#282a2b",
		"input.border": "#282a2b",
		"input.background": "#282a2b",
		
		/* Right click menu */
		"menu.background": "#101010",
		"menu.border": "#101010", // Only external border
		"menu.foreground": "#acacac",
		"menu.separatorBackground": "#3a3a3a",

		/* Status Bar (bottom of the screen)*/
		"statusBar.background": "#262626",
		"statusBar.foreground": "#868686",
			// Buttons
			"statusBarItem.hoverBackground": "#00000038",
				// When debugging
				"statusBar.debuggingBackground": "#fcce00",
				"statusBar.debuggingForeground": "#0b0b0b",
				// When no folder is selected
				"statusBar.noFolderBackground": "#262626",
				"statusBar.noFolderForeground": "#868686",
				// When remote
				"statusBarItem.remoteBackground": "#106399",
				"statusBarItem.remoteForeground": "#ffffff",
		
		/* Settings */
		"settings.focusedRowBackground": "#282d303d",
	},












	"tokenColors": [
		/**************************************************
		 * PHP
		 **************************************************/
		{
            "scope": [
				"source.php",
				"source.php punctuation",
				"source.php keyword.operator",
				"source.php string punctuation.section.array",
				"source.php punctuation.terminator",
            ],
			"settings": {"foreground": "#ffffff","fontStyle": ""}
        },

		{
            "scope": [
				"source.php meta.function-call support.function",
				"source.php support.function",
				"support.function.construct.output",
				"source.php keyword.control.exit",
				"source.php keyword.control.import.include",
            ],
			"settings": {"foreground": "#ebebeb","fontStyle": "bold italic"}
        },

		{
            "scope": [
				"source.php comment",
				"source.php comment keyword.other",
				"source.php comment punctuation",
				"source.php comment support.class punctuation.separator.inheritance",
				"source.php comment support.other.namespace",
				"source.php comment support.other.namespace punctuation.separator.inheritance",
				"source.php comment support.other",
				"source.php comment.block.documentation support.class",
				"source.php comment.block.documentation support punctuation",
				"source.php comment.block.documentation punctuation",
				"source.php comment.block.documentation punctuation.definition",
            ],
			"settings": {"foreground": "#95927e","fontStyle": ""}
        },

		{
            "scope": [
				"source.php meta.trait entity.name",
				"source.php meta.interface entity.name",
				"source.php meta.namespace entity.name",
				"source.php meta.namespace entity.name punctuation",
				"source.php meta.function entity.name",
				"source.php meta.function support.function.magic",
				"source.php meta.function support.function.constructor",
				"source.php meta.function punctuation.definition.variable",
				"source.php meta.class entity.name.type.class",
				"source.php meta.use entity.other.alias",
				"source.php meta.function variable",
            ],
			"settings": {"foreground": "#e7c500","fontStyle": "bold"}
        },

		{
            "scope": [
				"source.php storage.modifier.reference",
				"source.php keyword.operator.nullable-type",
            ],
			"settings": {"foreground": "#00e78f","fontStyle": "bold"}
        },

		{
            "scope": [
				"source.php constant.numeric",
				"source.php constant.numeric punctuation",
            ],
			"settings": {"foreground": "#ff6161","fontStyle": "bold"}
        },

		{
            "scope": [
				"source.php string",
				"source.php string punctuation",
				"source.php string keyword",
				"source.php string keyword.operator",
				"source.php string source.sql",
				"source.php string source.sql keyword.other",
				"source.php string source.sql constant.numeric",
				"source.php string source.sql support.function",
            ],
			"settings": {"foreground": "#ffa2ea","fontStyle": ""}
        },

		{
            "scope": [
				"source.php constant.language",
            ],
			"settings": {"foreground": "#b086ee","fontStyle": "bold"}
        },
		
		{
            "scope": [
				"source.php storage",
				"source.php keyword.other",
				"source.php meta.namespace",
				"source.php meta.function",
				"source.php meta.use keyword",
				"source.php meta.class storage",
				"source.php meta.class keyword.other.new",
				"source.php keyword.control",
				"source.php keyword.other.new",
				"source.php keyword.operator.type",
				"source.php meta.function.parameter.typehinted punctuation.separator.delimiter",
				"source.php meta.function.parameter.typehinted storage.type",
            ],
			"settings": {"foreground": "#80b7ff","fontStyle": ""}
        },

		{
            "scope": [
				"source.php keyword.operator.logical",
            ],
			"settings": {"foreground": "#80b7ff","fontStyle": "underline"}
        },

		{
            "scope": [
				"source.php variable",
				"source.php variable punctuation.definition",
            ],
			"settings": {"foreground": "#46d4ff","fontStyle": ""}
        },

		{
            "scope": [
				"source.php support.class",
				"source.php meta.other.inherited-class",
				"source.php meta.method-call entity.name.function",
				"source.php meta.function-call entity.name.function",
				"source.php meta.function-call support.other.namespace",
				"source.php support.class punctuation.separator.inheritance",
				"source.php support.other.namespace",
				"source.php support.other.namespace punctuation.separator.inheritance",
				"source.php meta.function-call support.class",
				"source.php string.unquoted.index",
				"source.php meta.use support",
				"source.php meta.use support punctuation",
				"source.php meta.class support",
				"source.php meta.class support punctuation",
				"source.php meta.method-call",
				"source.php variable.other.property",
            ],
			"settings": {"foreground": "#aaeaaa","fontStyle": ""}
        },

		{
            "scope": [
				"source.php constant.other",
				"source.php support.constant",
            ],
			"settings": {"foreground": "#7ece7e","fontStyle": "bold"}
        },
		


















		

		
