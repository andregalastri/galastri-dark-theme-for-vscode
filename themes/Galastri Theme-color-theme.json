{
	"name": "Galastri Dark Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#161616ff",
		"editor.foreground": "#eaeaeaff",
		"activityBarBadge.background": "#91d3ff",
		"sideBarTitle.foreground": "#bbbbbbff",
		"editor.selectionBackground": "#2b445e",
		"editor.selectionHighlightBackground": "#38608b",
		"editorBracketMatch.background": "#e727cde8",
		"editorBracketMatch.border": "#f50bd600",
		"editorLineNumber.foreground": "#666666",
		"editor.lineHighlightBorder": "#00000000",
		"editor.lineHighlightBackground": "#2233444b",
		"editorIndentGuide.activeBackground": "#292d33",
		"editorIndentGuide.background": "#1a1d20",
		"editor.foldBackground": "#00000000",
		"editor.findMatchHighlightBackground": "#fff88f",
		"statusBar.background": "#262626",
		"statusBar.foreground": "#808080"
	},
	"tokenColors": [
		{
			"scope": [
				"punctuation",
				"keyword.operator",
				"meta.brace",
				"meta.property-name support.type.vendored",
				"meta.selector entity.other.attribute-name.class",
				"meta.selector entity.other.attribute-name.id",
				"text.html",
				"text.xml"
			],
			"settings": {
				"foreground": "#eaeaeaff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"meta.method.declaration.js",
				"meta.class variable.other.property",
				"meta.object.member meta.object-literal.key",
				"meta.function-call variable.other.object.property",
				"meta.function-call entity.name.function",
				"variable.other.object.property.js",
				"variable.other.property.js",
				"meta.property-name",
				"source.css support.type.property-name",
				"keyword.other.sql"
			],
			"settings": {
				"foreground": "#c7e589ff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"meta.selector",
				"text.html entity.name.tag.html",
				"text.html punctuation.definition.tag.begin.html",
				"text.html punctuation.definition.tag.end.html",
				"text.xml punctuation.definition.tag.xml",
				"text.xml entity.name.tag.localname",
				"source.json.comments support.type.property-name",
				"source.json.comments punctuation.support.type.property-name",
				"meta.selector entity.name.tag",
			],
			"settings": {
				"foreground": "#ff61bcff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"text.html entity.other.attribute-name.html",
				"text.xml entity.other.attribute-name.localname.xml",
				"entity.other.attribute-name",
			],
			"settings": {
				"foreground": "#8ae5c9ff",
				"fontStyle": "",
			}
		},
		
		
		{
			"scope": [
				"meta.use support.other.namespace",
				"meta.use punctuation.separator.inheritance",
				"meta.use support.class",
				"meta.class punctuation.separator.inheritance",
				"meta.class meta.other.inherited-class.php",
				"source.js support.function",
				"meta.class support.class.php",
				"meta.class meta.method-call.static",
				"support.class.builtin.php",
				"support.class.exception.php",
				"constant",
				"string.unquoted.index",
				"support.constant.ext",
				"source.js entity.name.type",
				"source.php variable.other.property",
				"source.php meta.method-call",
				"source.php support.class.php",
				"meta.use entity.other.alias",
				"source.php support.constant"
			],
			"settings": {
				"foreground": "#aaeaa8ff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"meta.class",
				"meta.function",
				"meta.namespace",
				"meta.use",
				"storage.type.property",
				"meta.class variable.language.this",
				"meta.class keyword.operator.expression.typeof",
				"keyword.control",
				"source.js",
				"source.php keyword.control.return",
				"source.js keyword.control.flow",
				"source.css keyword.other.important",
				"source.js keyword.operator.new",
				"keyword.operator.expression.typeof",
				"keyword.operator.expression.in",
				"meta.method.declaration.js meta.var.expr.js",
				"source.php keyword.operator.logical",
				"source.php storage.modifier.php",
				"source.php storage.type.php",
				"source.php keyword.other"
			],
			"settings": {
				"foreground": "#91c0ffff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"variable",
				"punctuation.definition.variable.php",
				"meta.selector entity.other.attribute-name.pseudo-element",
				"meta.selector entity.other.attribute-name.pseudo-class",
				"source.sql text.variable",
				"variable.language.this.php"
			],
			"settings": {
				"foreground": "#64e0ff",
				"fontStyle": "bold",
			}
		},

		{
			"scope": [
				"meta.definition.variable entity.name",
				"variable.parameter",
				"meta.function.parameter variable.other",
				"meta.function.parameter punctuation.definition.variable.php",
				"entity.name.type.namespace punctuation.separator.inheritance",
				"meta.definition.variable variable",
				"meta.namespace entity.name.type.namespace",
				"meta.class.php entity.name.type.class",
				"meta.function.php entity.name.function.php",
				"meta.class.js entity.name.type.class",
				"source.css keyword.control.at-rule",
				"source.css punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#ffd800ff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"constant.language",
				"meta.property-list",
				"source.css support.function",
				"source.css constant.other.color",
				"keyword.other.DDL.create.II.sql"
			],
			"settings": {
				"foreground": "#ccaaffff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string.regexp punctuation",
				"string.regexp keyword.operator",
				"string.regexp constant",
				"source.php keyword.operator.heredoc",
				"source.css variable.parameter",
				"string.quoted constant",
				"string.quoted keyword",
				"string.quoted keyword.operator",
				"string.quoted keyword.other.sql",
				"source.php string.quoted keyword.other.DML.sql",
				"source.php string.quoted keyword.other.alias.sql",
				"string.json punctuation",
			],
			"settings": {
				"foreground": "#ff97e7ff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"support.function",
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#ff7272ff",
				"fontStyle": "",
			}
		},

		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.documentation.phpdoc",
				"comment.block.documentation keyword.other",
				"meta.class comment.block.documentation.phpdoc support.class.php"
			],
			"settings": {
				"foreground": "#95927eff",
				"fontStyle": "",
			}
		},
	]
}